{
  "name": "secretaria-pro---organizer",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\" --names \"FRONTEND,BACKEND\" --prefix-colors \"cyan,magenta\"",
    "dev:frontend": "vite",
    "dev:backend": "cd server && npm run dev",
    "build": "vite build",
    "build:backend": "cd server && npm run build",
    "build:all": "npm run build:backend && npm run build",
    "preview": "vite preview",
    "kill": "node scripts/kill.cjs",
    "stop": "npm run kill"
  },
  "dependencies": {
    "react-dom": "^19.2.0",
    "react": "^19.2.0",
    "lucide-react": "^0.554.0",
    "axios": "^1.7.7",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.1"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "@vitejs/plugin-react": "^5.0.0",
    "typescript": "~5.8.2",
    "vite": "^6.2.0",
    "vite-plugin-pwa": "^0.21.1",
    "concurrently": "^9.1.0"
  }
}
